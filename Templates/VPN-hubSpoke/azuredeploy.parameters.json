{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vpnType": {
            "value": "RouteBased",
            "metadata": {
                "description":"Azure VPN Type"
            }
        },
        "localGatewayName": {
            "value": "onPremGateway"
        },
        "localGatewayIpAddress": {
            "value": "1.1.1.1",
            "metadata": {
                "description":"Public IP for On-Prem VPN gateway"
            }            
        },
        "localAddressPrefix": {
            "value": [ "192.168.0.0/16", "172.16.0.0/12" ],
            "metadata": {
                "description":"On-prem network(s)"
            }              
        },
        "virtualNetworkName": {
            "value": "wings-core-vnet"
        },      
        "gatewaySubnetPrefix": {
             "value": "10.3.200.0/29"
        },
        "gatewayName": {
            "value": "vpnGateway"
        },
        "connectionName": {
            "value": "onPremConnection"
        },
        "sharedKey": {
            "value": "",
            "metadata": {
                "description":"Pre-Shared key"
            }            
        }
    }
}