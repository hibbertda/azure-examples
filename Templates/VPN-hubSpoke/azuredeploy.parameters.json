{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vpnConfig": {
            "metadata": {
                "description": "configuration information for S2S VPN"
            },
            "value": [
                {
                    "vpnType": "RouteBased",
                    "gatewayName": "WingsS2SGW",
                    "gatewaySku": "Basic"
                }
            ]
        },
        "azureVNetAddressPrefix": {
            "metadata": {
                "description": "VNet IP address Prefix"
            },
            "value": "10.4.0.0/16"
        },
        "subnetsList": {
            "metadata": {
                "description": "List of subnets to create in primary VNet."
            },
            "value": [
                {
                    "name": "CoreServices",
                    "addressPrefix": "10.4.1.0/24",
                    "nsg": "true"
                },
                {
                    "name": "SharedServices",
                    "addressPrefix": "10.4.2.0/24",
                    "nsg": "true"
                },
                {
                    "name": "DirectoryServices",
                    "addressPrefix": "10.4.3.0/24",
                    "nsg": "true"
                }                            
            ]
        },
        "gatewaySubnet": {
            "value": [
                {
                    "name": "GatewaySubnet",
                    "addressPrefix": "10.4.250.0/28"
                }
            ]
        },
        "localGatewayName": {
            "value": "onPremGateway"
        },
        "localGatewayIpAddress": {
            "value": "1.1.1.1",
            "metadata": {
                "description":"Public IP for On-Prem VPN gateway"
            }            
        },
        "localAddressPrefix": {
            "value": [ "192.168.0.0/16", "172.16.0.0/12" ],
            "metadata": {
                "description":"On-prem network(s)"
            }              
        },
        "virtualNetworkName": {
            "value": "wings-core-vnet"
        },      
        "connectionName": {
            "value": "onPremConnection"
        },
        "sharedKey": {
            "value": "",
            "metadata": {
                "description":"Pre-Shared key"
            }            
        }
    }
}